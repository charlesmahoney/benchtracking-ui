#!/bin/bash
echo "Before assembling"
echo "THIS IS CHARLES>>> FIND ME"
nvm ls
echo "Assemble file:"
cat /usr/libexec/s2i/assemble
echo "Assemble file END"

npm install -g n

/usr/libexec/s2i/assemble
rc=$?

if [ $rc -eq 0 ]; then
    echo "After successful assembling"
else
    echo "After failed assembling"
fi

exit $rc